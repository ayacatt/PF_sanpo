<div class='main-container'>
  <div class='row'>
    <div class='col-md-4 offset-md-1'>
      <div class=main-subject>市区町村登録</div>
        <%= form_with model: @genre, url:admin_genres_path,local: true do |f| %>
        <div class="form-group">
          <%= f.label :市区町村名 %>
          <%= f.text_field :name, class: 'form-control name' %>
        </div>
          <%= f.submit "新規登録" ,class: 'btn btn-outline-success'%>
        <% end %>
    </div>
  </div>
</div>
<div class='main-container'>
  <div class='row'>
    <div class='col-md-8 offset-md-1'>
      <div class=main-subject>市区町村一覧</div>
      <%= render 'index', genres: @genres, genre: @genre %>
    </div>
  </div>
</div>